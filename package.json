{
  "name": "next11-reaxt-native-v2",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --dev-client",
    "start:offline": "npx expo start --dev-client --offline",
    "start:prod": "npx expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint:fix": "npx eslint src --fix --ext .js,.ts,.tsx",
    "lint-stagged:fix": "npx eslint src --fix --cache --ext .js,.ts,.tsx",
    "lint": "npx eslint src --ext .js,.ts,.tsx",
    "build-locally": "npx eas build --profile development-simulator --platform ios --local",
    "build:dev": "npx eas build --profile testing --platform ios --clear-cache",
    "build:stg": "npx eas build --profile stagging --platform ios --clear-cache",
    "build:prod": "npx eas build -p ios --clear-cache",
    "submit:prod": "npx eas submit --profile production --platform ios --latest",
    "submit:dev": "npx eas submit --profile testing --platform ios --latest",
    "submit:stg": "npx eas submit --profile staging --platform ios --latest",
    "prepare": "husky install",
    "update-dev": "npx eas update --branch testing",
    "update-stg": "npx eas update --branch staging",
    "update-prod": "npx eas update --branch production",
    "check-updates": "npx npm-check-updates",
    "tsc": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
